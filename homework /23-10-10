CREATE TABLE DEPARTMENT (DNO VARCHAR2(10) PRIMARY KEY, DNAME VARCHAR2(20), CATEGORY VARCHAR2(20), OPEN_YN CHAR(1), CAPACITY NUMBER);
CREATE TABLE STUDENT (
SNO VARCHAR2(10) PRIMARY KEY,
DNO VARCHAR(10),
S_SSN VARCHAR2(14),
SADDRESS VARCHAR2(100),
ENTRACE_DATE DATE,
ABSENCE_YN CHAR(1),
PNO VARCHAR2(10));

CREATE TABLE CLASS (CNO VARCHAR2(10) PRIMARY KEY, DNO VARCHAR2(10), PREATTENDIN_NO VARCHAR2(10), CNAME VARCHAR2(30), CTYPE VARCHAR2(10));

CREATE TABLE CLASS_PROFESSOR ( CNO VARCHAR2(10) PRIMARY KEY, PNO VARCHAR2(10) PRIMARY KEY);

CREATE TABLE PROFESSOR (PNO VARCHAR2(10) PRIMARY KEY, PNAME VARCHAR2(30), P_SSN VARCHAR2(14), PADDRESS VARCHAR2(100), DNO VARCHAR2(10));

CREATE TABLE GRADE (TNO VARCHAR2(10) PRIMARY KEY, CNO VARCHAR2(10) PRIMARY KEY, SNO VARCHAR2(10) PRIMARY KEY, POINT NUMBER(3,2));
